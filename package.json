    "dev:server": "npm run kill:3000 && PORT=3000 NODE_ENV=development tsx server/index.ts",
    "dev:all": "concurrently \"npm run dev\" \"npm run dev:server\"",
    "start:all": "./start-all.sh",
    "build": "npx vite build",
    "build:server": "esbuild api/start.ts --bundle --platform=node --packages=external --format=cjs --outfile=dist/server.cjs",
    "start:server": "node dist/server.cjs",
    "lint": "eslint . --ext ts,tsx --report-unused-disable-directives --max-warnings 0",
    "start": "NODE_ENV=production node dist/server.cjs",
    "check": "tsc",
    "db:push": "drizzle-kit push",
    "build:dev": "vite build --mode development",
    "build:all": "npm run build && npm run build:server",
